generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Company {
  id                    String                 @id @default(uuid())
  code                  String                 @unique
  name                  String
  address               String?
  phone                 String?
  email                 String?
  website               String?
  logo                  String?
  isActive              Boolean                @default(true)
  createdAt             DateTime               @default(now())
  updatedAt             DateTime               @updatedAt
  accounts              Account[]
  employees             Employee[]
  fakturs               Faktur[]
  inventoryTransactions InventoryTransaction[]
  items                 Item[]
  itemCategories        ItemCategory[]
  journalEntries        JournalEntry[]
  paymentTerms          PaymentTerm[]
  printTemplates        PrintTemplate[]
  salesOrders           SalesOrder[]
  salesReceipts         SalesReceipt[]
  salespersons          Salesperson[]
  units                 Unit[]
  users                 User[]
  warehouses            Warehouse[]
  customers             Customer[]
}

model User {
  id        String   @id @default(uuid())
  companyId String
  email     String   @unique
  password  String
  name      String
  role      UserRole @default(STAFF)
  isActive  Boolean  @default(true)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  company   Company  @relation(fields: [companyId], references: [id])

  @@index([companyId])
  @@index([isActive, companyId])
}

model Customer {
  id                     String         @id @default(uuid())
  companyId              String
  code                   String
  name                   String
  category               String?
  contactPerson          String?
  email                  String?
  phone                  String?
  mobile                 String?
  fax                    String?
  website                String?
  currency               String         @default("IDR")
  notes                  String?
  billingAddress         String?
  billingCity            String?
  billingProvince        String?
  billingCountry         String?
  billingZipCode         String?
  shippingAddress        String?
  shippingCity           String?
  shippingProvince       String?
  shippingCountry        String?
  shippingZipCode        String?
  priceCategory          String?
  discountCategory       String?
  paymentTerms           Int            @default(0)
  creditLimit            Decimal        @default(0) @db.Decimal(15, 2)
  maxReceivableDays      Int            @default(0)
  salesperson            String?
  defaultDiscount        Decimal        @default(0) @db.Decimal(5, 2)
  taxIncluded            Boolean        @default(false)
  npwp                   String?
  taxName                String?
  nik                    String?
  nppkp                  String?
  taxAddress             String?
  transactionDetail      String?
  documentType           String?
  receivableAccountId    String?
  downPaymentAccountId   String?
  salesAccountId         String?
  cogsAccountId          String?
  salesReturnAccountId   String?
  goodsDiscountAccountId String?
  salesDiscountAccountId String?
  isActive               Boolean        @default(true)
  createdAt              DateTime       @default(now())
  updatedAt              DateTime       @updatedAt
  paymentTermId          String?
  fakturs                Faktur[]
  itemSuppliers          ItemSupplier[]
  salesOrders            SalesOrder[]
  salesReceipts          SalesReceipt[]
  cogsAccount            Account?       @relation("CustCOGS", fields: [cogsAccountId], references: [id])
  company                Company        @relation(fields: [companyId], references: [id])
  downPaymentAccount     Account?       @relation("CustDownPayment", fields: [downPaymentAccountId], references: [id])
  goodsDiscountAccount   Account?       @relation("CustGoodsDisc", fields: [goodsDiscountAccountId], references: [id])
  paymentTerm            PaymentTerm?   @relation(fields: [paymentTermId], references: [id])
  receivableAccount      Account?       @relation("CustReceivable", fields: [receivableAccountId], references: [id])
  salesAccount           Account?       @relation("CustSales", fields: [salesAccountId], references: [id])
  salesDiscountAccount   Account?       @relation("CustSalesDisc", fields: [salesDiscountAccountId], references: [id])
  salesReturnAccount     Account?       @relation("CustReturn", fields: [salesReturnAccountId], references: [id])

  @@unique([companyId, code])
  @@index([companyId])
  @@index([paymentTermId])
  @@index([isActive, companyId])
  @@map("customer")
}

model Item {
  id                    String                 @id @default(uuid())
  companyId             String
  code                  String
  name                  String
  description           String?
  barcode               String?                @unique
  uom                   String                 @default("PCS")
  brand                 String?
  categoryId            String?
  isStockItem           Boolean                @default(true)
  serialNumberActive    Boolean                @default(false)
  isActive              Boolean                @default(true)
  createdAt             DateTime               @default(now())
  updatedAt             DateTime               @updatedAt
  fakturLines           FakturLine[]
  inventoryTransactions InventoryTransaction[]
  category              ItemCategory?          @relation(fields: [categoryId], references: [id])
  company               Company                @relation(fields: [companyId], references: [id])
  accounts              ItemAccount[]
  pricing               ItemPricing[]
  stocks                ItemStock[]
  suppliers             ItemSupplier[]
  taxes                 ItemTax[]
  salesOrderLines       SalesOrderLine[]

  @@unique([companyId, code])
  @@index([categoryId])
  @@index([barcode])
  @@index([companyId])
  @@index([isActive, companyId])
}

model ItemCategory {
  id           String         @id @default(uuid())
  companyId    String
  code         String
  name         String
  description  String?
  parentId     String?
  productType  String?
  hppAccountId String?
  isActive     Boolean        @default(true)
  createdAt    DateTime       @default(now())
  updatedAt    DateTime       @updatedAt
  items        Item[]
  company      Company        @relation(fields: [companyId], references: [id])
  hppAccount   Account?       @relation("CategoryHPP", fields: [hppAccountId], references: [id])
  parent       ItemCategory?  @relation("CategoryHierarchy", fields: [parentId], references: [id])
  children     ItemCategory[] @relation("CategoryHierarchy")

  @@unique([companyId, code])
  @@index([parentId])
  @@index([companyId])
}

model Unit {
  id          String   @id @default(uuid())
  companyId   String
  name        String
  description String?
  isActive    Boolean  @default(true)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  company     Company  @relation(fields: [companyId], references: [id])

  @@unique([companyId, name])
}

model PaymentTerm {
  id        String     @id @default(uuid())
  companyId String
  code      String
  name      String
  days      Int        @default(0)
  isActive  Boolean    @default(true)
  createdAt DateTime   @default(now())
  updatedAt DateTime   @updatedAt
  fakturs   Faktur[]
  company   Company    @relation(fields: [companyId], references: [id])
  customers Customer[]

  @@unique([companyId, code])
  @@index([companyId])
}

model ItemPricing {
  id              String    @id @default(uuid())
  itemId          String
  priceType       PriceType
  price           Decimal   @default(0) @db.Decimal(15, 2)
  defaultDiscount Decimal   @default(0) @db.Decimal(5, 2)
  minimumQuantity Decimal   @default(0) @db.Decimal(10, 2)
  currency        String    @default("IDR")
  isActive        Boolean   @default(true)
  effectiveFrom   DateTime?
  effectiveTo     DateTime?
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  item            Item      @relation(fields: [itemId], references: [id], onDelete: Cascade)

  @@index([itemId, priceType])
  @@index([effectiveFrom, effectiveTo])
}

model Salesperson {
  id          String       @id @default(uuid())
  companyId   String
  code        String
  name        String
  phone       String?
  email       String?
  isActive    Boolean      @default(true)
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt
  employeeId  String?      @unique
  fakturs     Faktur[]
  fakturLines FakturLine[]
  company     Company      @relation(fields: [companyId], references: [id])
  employee    Employee?    @relation(fields: [employeeId], references: [id], onDelete: Cascade)

  @@unique([companyId, code])
  @@index([companyId])
}

model Employee {
  id             String       @id @default(uuid())
  companyId      String
  fullName       String
  salutation     String?
  position       String?
  email          String?
  mobilePhone    String?
  businessPhone  String?
  homePhone      String?
  whatsapp       String?
  website        String?
  citizenship    String?
  employeeIdType String?
  idCardNumber   String?
  joinDate       DateTime?
  branch         String?
  notes          String?
  isActive       Boolean      @default(true)
  createdAt      DateTime     @default(now())
  updatedAt      DateTime     @updatedAt
  company        Company      @relation(fields: [companyId], references: [id])
  salesperson    Salesperson?

  @@index([companyId])
  @@index([isActive, companyId])
}

model Warehouse {
  id                    String                 @id @default(uuid())
  companyId             String
  code                  String
  name                  String
  address               String?
  city                  String?
  isActive              Boolean                @default(true)
  createdAt             DateTime               @default(now())
  updatedAt             DateTime               @updatedAt
  fakturLines           FakturLine[]
  inventoryTransactions InventoryTransaction[]
  stocks                ItemStock[]
  company               Company                @relation(fields: [companyId], references: [id])

  @@unique([companyId, code])
  @@index([companyId])
  @@index([isActive, companyId])
}

model ItemStock {
  id             String    @id @default(uuid())
  itemId         String
  warehouseId    String
  minStock       Decimal   @default(0) @db.Decimal(10, 2)
  maxStock       Decimal   @default(0) @db.Decimal(10, 2)
  currentStock   Decimal   @default(0) @db.Decimal(10, 2)
  reservedStock  Decimal   @default(0) @db.Decimal(10, 2)
  availableStock Decimal   @default(0) @db.Decimal(10, 2)
  reorderPoint   Decimal   @default(0) @db.Decimal(10, 2)
  lastStockCount DateTime?
  updatedAt      DateTime  @updatedAt
  item           Item      @relation(fields: [itemId], references: [id], onDelete: Cascade)
  warehouse      Warehouse @relation(fields: [warehouseId], references: [id])

  @@unique([itemId, warehouseId])
  @@index([availableStock])
  @@index([itemId])
  @@index([warehouseId])
  @@index([warehouseId, availableStock])
}

model InventoryTransaction {
  id              String          @id @default(uuid())
  companyId       String
  itemId          String
  warehouseId     String
  transactionDate DateTime
  transactionType TransactionType
  referenceNo     String?
  quantity        Decimal         @db.Decimal(10, 2)
  unit            String
  costPerUnit     Decimal         @default(0) @db.Decimal(15, 2)
  totalCost       Decimal         @default(0) @db.Decimal(15, 2)
  notes           String?
  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt
  createdBy       String?
  company         Company         @relation(fields: [companyId], references: [id])
  item            Item            @relation(fields: [itemId], references: [id])
  warehouse       Warehouse       @relation(fields: [warehouseId], references: [id])

  @@index([companyId])
  @@index([itemId])
  @@index([warehouseId])
  @@index([transactionDate])
}

model ItemSupplier {
  id                String    @id @default(uuid())
  itemId            String
  supplierId        String
  isPrimary         Boolean   @default(false)
  purchaseUom       String?
  purchasePrice     Decimal   @default(0) @db.Decimal(15, 2)
  leadTimeDays      Int       @default(0)
  minimumOrder      Decimal   @default(0) @db.Decimal(10, 2)
  lastPurchaseDate  DateTime?
  lastPurchasePrice Decimal?  @db.Decimal(15, 2)
  isActive          Boolean   @default(true)
  createdAt         DateTime  @default(now())
  updatedAt         DateTime  @updatedAt
  item              Item      @relation(fields: [itemId], references: [id], onDelete: Cascade)
  supplier          Customer  @relation(fields: [supplierId], references: [id])

  @@unique([itemId, supplierId])
  @@index([isPrimary])
  @@index([itemId])
  @@index([itemId, isPrimary])
}

model ItemTax {
  id           String   @id @default(uuid())
  itemId       String
  taxCode      String
  taxName      String
  taxRate      Decimal  @default(0) @db.Decimal(5, 2)
  taxAccountId String?
  isActive     Boolean  @default(true)
  createdAt    DateTime @default(now())
  item         Item     @relation(fields: [itemId], references: [id], onDelete: Cascade)
  taxAccount   Account? @relation(fields: [taxAccountId], references: [id])

  @@index([itemId, taxCode])
}

model ItemAccount {
  id          String          @id @default(uuid())
  itemId      String
  accountType ItemAccountType
  accountId   String
  createdAt   DateTime        @default(now())
  account     Account         @relation(fields: [accountId], references: [id])
  item        Item            @relation(fields: [itemId], references: [id], onDelete: Cascade)

  @@unique([itemId, accountType])
}

model SalesOrder {
  id              String           @id @default(uuid())
  companyId       String
  orderNumber     String
  orderDate       DateTime
  dueDate         DateTime?
  customerId      String
  status          OrderStatus      @default(DRAFT)
  currency        String           @default("IDR")
  subtotal        Decimal          @db.Decimal(15, 2)
  discountPercent Decimal          @default(0) @db.Decimal(5, 2)
  discountAmount  Decimal          @default(0) @db.Decimal(15, 2)
  taxPercent      Decimal          @default(0) @db.Decimal(5, 2)
  taxAmount       Decimal          @default(0) @db.Decimal(15, 2)
  totalAmount     Decimal          @db.Decimal(15, 2)
  notes           String?
  createdBy       String?
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
  fakturs         Faktur[]
  company         Company          @relation(fields: [companyId], references: [id])
  customer        Customer         @relation(fields: [customerId], references: [id])
  lines           SalesOrderLine[]

  @@unique([companyId, orderNumber])
  @@index([companyId])
  @@index([customerId])
  @@index([status])
  @@index([orderDate])
  @@index([companyId, status])
}

model SalesOrderLine {
  id              String     @id @default(uuid())
  salesOrderId    String
  itemId          String?
  description     String
  quantity        Decimal    @db.Decimal(10, 2)
  unitPrice       Decimal    @db.Decimal(15, 2)
  discountPercent Decimal    @default(0) @db.Decimal(5, 2)
  amount          Decimal    @db.Decimal(15, 2)
  notes           String?
  item            Item?      @relation(fields: [itemId], references: [id])
  salesOrder      SalesOrder @relation(fields: [salesOrderId], references: [id], onDelete: Cascade)

  @@index([salesOrderId])
  @@index([itemId])
}

model Faktur {
  id              String             @id @default(uuid())
  companyId       String
  fakturNumber    String
  salesOrderId    String?
  fakturDate      DateTime
  dueDate         DateTime?
  customerId      String
  status          FakturStatus       @default(DRAFT)
  currency        String             @default("IDR")
  subtotal        Decimal            @db.Decimal(15, 2)
  discountPercent Decimal            @default(0) @db.Decimal(5, 2)
  discountAmount  Decimal            @default(0) @db.Decimal(15, 2)
  taxPercent      Decimal            @default(0) @db.Decimal(5, 2)
  taxAmount       Decimal            @default(0) @db.Decimal(15, 2)
  totalAmount     Decimal            @db.Decimal(15, 2)
  amountPaid      Decimal            @default(0) @db.Decimal(15, 2)
  balanceDue      Decimal            @db.Decimal(15, 2)
  notes           String?
  createdBy       String?
  createdAt       DateTime           @default(now())
  updatedAt       DateTime           @updatedAt
  paymentTermId   String?
  paymentTerms    String?
  salespersonId   String?
  shippingDate    DateTime?
  taxInclusive    Boolean            @default(true)
  address         String?                              // Alamat pengiriman/penagihan
  totalCost       Decimal            @default(0) @db.Decimal(15, 2) // Total biaya lainnya
  company         Company            @relation(fields: [companyId], references: [id])
  customer        Customer           @relation(fields: [customerId], references: [id])
  paymentTerm     PaymentTerm?       @relation(fields: [paymentTermId], references: [id])
  salesOrder      SalesOrder?        @relation(fields: [salesOrderId], references: [id])
  salesperson     Salesperson?       @relation(fields: [salespersonId], references: [id])
  costs           FakturCost[]
  lines           FakturLine[]
  receiptLines    SalesReceiptLine[]

  @@unique([companyId, fakturNumber])
  @@index([companyId])
  @@index([customerId])
  @@index([salespersonId])
  @@index([status])
  @@index([fakturDate])
  @@index([dueDate])
  @@index([status, dueDate])
  @@index([customerId, status])
  @@index([fakturDate, status])
  @@index([salespersonId, fakturDate])
  @@index([salesOrderId])
  @@index([paymentTermId])
}

model FakturLine {
  id              String       @id @default(uuid())
  fakturId        String
  itemId          String?
  description     String?
  quantity        Decimal      @db.Decimal(10, 2)
  unitPrice       Decimal      @db.Decimal(15, 2)
  discountPercent Decimal      @default(0) @db.Decimal(5, 2)
  amount          Decimal      @db.Decimal(15, 2)
  warehouseId     String?
  discountAmount  Decimal      @default(0) @db.Decimal(15, 2)
  unit            String?
  itemName        String
  salespersonId   String?
  subtotalBeforeDiscount Decimal @default(0) @db.Decimal(15, 2) // unitPrice x quantity
  customerId      String?                                        // Customer ID untuk laporan
  faktur          Faktur       @relation(fields: [fakturId], references: [id], onDelete: Cascade)
  item            Item?        @relation(fields: [itemId], references: [id])
  salesperson     Salesperson? @relation(fields: [salespersonId], references: [id])
  warehouse       Warehouse?   @relation(fields: [warehouseId], references: [id])

  @@index([fakturId])
  @@index([itemId])
  @@index([warehouseId])
  @@index([salespersonId])
}

model FakturCost {
  id        String   @id @default(uuid())
  fakturId  String
  accountId String
  amount    Decimal  @db.Decimal(15, 2)
  notes     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  account   Account  @relation("FakturCostAccount", fields: [accountId], references: [id])
  faktur    Faktur   @relation(fields: [fakturId], references: [id], onDelete: Cascade)

  @@index([fakturId])
  @@index([accountId])
}

model SalesReceipt {
  id            String             @id @default(uuid())
  companyId     String
  receiptNumber String
  receiptDate   DateTime
  customerId    String
  bankAccountId String
  paymentMethod String             @default("TRANSFER")
  amount        Decimal            @db.Decimal(15, 2)
  notes         String?
  status        ReceiptStatus      @default(DRAFT)
  createdBy     String?
  createdAt     DateTime           @default(now())
  updatedAt     DateTime           @updatedAt
  bankAccount   Account            @relation(fields: [bankAccountId], references: [id])
  company       Company            @relation(fields: [companyId], references: [id])
  customer      Customer           @relation(fields: [customerId], references: [id])
  lines         SalesReceiptLine[]

  @@unique([companyId, receiptNumber])
  @@index([companyId])
  @@index([customerId])
  @@index([receiptDate])
  @@index([status])
  @@index([bankAccountId])
  @@index([companyId, status])
  @@index([customerId, receiptDate])
}

model SalesReceiptLine {
  id             String       @id @default(uuid())
  salesReceiptId String
  fakturId       String?
  amount         Decimal      @db.Decimal(15, 2)
  faktur         Faktur?      @relation(fields: [fakturId], references: [id])
  salesReceipt   SalesReceipt @relation(fields: [salesReceiptId], references: [id], onDelete: Cascade)

  @@index([salesReceiptId])
  @@index([fakturId])
}

model Account {
  id              String           @id @default(uuid())
  companyId       String
  code            String
  name            String
  parentId        String?
  type            AccountType
  classification  String?
  level           Int              @default(0)
  isHeader        Boolean          @default(false)
  currency        String           @default("IDR")
  isActive        Boolean          @default(true)
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt
  company         Company          @relation(fields: [companyId], references: [id])
  parent          Account?         @relation("AccountHierarchy", fields: [parentId], references: [id])
  children        Account[]        @relation("AccountHierarchy")
  balances        AccountBalance[]
  fakturCosts     FakturCost[]     @relation("FakturCostAccount")
  itemAccounts    ItemAccount[]
  hppCategories   ItemCategory[]   @relation("CategoryHPP")
  itemTaxes       ItemTax[]
  journalLines    JournalLine[]
  salesReceipts   SalesReceipt[]
  custCOGS        Customer[]       @relation("CustCOGS")
  custDownPayment Customer[]       @relation("CustDownPayment")
  custGoodsDisc   Customer[]       @relation("CustGoodsDisc")
  custReceivable  Customer[]       @relation("CustReceivable")
  custSales       Customer[]       @relation("CustSales")
  custSalesDisc   Customer[]       @relation("CustSalesDisc")
  custReturn      Customer[]       @relation("CustReturn")

  @@unique([companyId, code])
  @@index([companyId, type])
  @@index([companyId])
  @@index([parentId])
  @@index([type])
  @@index([isActive, companyId])
  @@index([isHeader, companyId])
}

model JournalEntry {
  id              String        @id @default(uuid())
  companyId       String
  transactionDate DateTime
  transactionNo   String
  reference       String?
  description     String?
  sourceType      String?
  sourceId        String?
  createdAt       DateTime      @default(now())
  updatedAt       DateTime      @updatedAt
  company         Company       @relation(fields: [companyId], references: [id])
  lines           JournalLine[]

  @@unique([companyId, transactionNo])
  @@index([transactionDate])
  @@index([companyId])
  @@index([transactionDate, companyId])
  @@index([sourceType])
  @@index([sourceId])
}

model JournalLine {
  id          String       @id @default(uuid())
  journalId   String
  accountId   String
  description String?
  debit       Decimal      @default(0) @db.Decimal(15, 2)
  credit      Decimal      @default(0) @db.Decimal(15, 2)
  account     Account      @relation(fields: [accountId], references: [id])
  journal     JournalEntry @relation(fields: [journalId], references: [id], onDelete: Cascade)

  @@index([accountId, journalId])
  @@index([journalId])
  @@index([accountId])
}

model AccountBalance {
  id             String   @id @default(uuid())
  accountId      String
  period         DateTime
  openingBalance Decimal  @default(0) @db.Decimal(15, 2)
  totalDebit     Decimal  @default(0) @db.Decimal(15, 2)
  totalCredit    Decimal  @default(0) @db.Decimal(15, 2)
  closingBalance Decimal  @default(0) @db.Decimal(15, 2)
  account        Account  @relation(fields: [accountId], references: [id])

  @@unique([accountId, period])
  @@index([accountId])
  @@index([period])
}

model PrintTemplate {
  id        String            @id @default(uuid())
  companyId String
  name      String
  type      PrintTemplateType
  content   String?
  isDefault Boolean           @default(false)
  isActive  Boolean           @default(true)
  createdAt DateTime          @default(now())
  updatedAt DateTime          @updatedAt
  company   Company           @relation(fields: [companyId], references: [id])

  @@index([companyId])
  @@index([type])
}

enum UserRole {
  ADMIN
  MANAGER
  STAFF
}

enum PriceType {
  SELL
  PURCHASE
  WHOLESALE
  RETAIL
  SPECIAL
}

enum TransactionType {
  INITIAL
  ADJ_IN
  ADJ_OUT
  PURCHASE
  SALE
  TRANSFER_IN
  TRANSFER_OUT
  STOCK_OPNAME
}

enum ItemAccountType {
  INVENTORY
  SALES
  SALES_RETURN
  SALES_DISCOUNT
  GOODS_SHIPPED
  COGS
  PURCHASE_RETURN
  PURCHASE_ACCRUAL
}

enum OrderStatus {
  DRAFT
  CONFIRMED
  PROCESSING
  COMPLETED
  CANCELLED
}

enum FakturStatus {
  DRAFT
  PENDING
  REJECTED
  UNPAID
  PARTIAL
  PAID
  OVERDUE
  CANCELLED
}

enum ReceiptStatus {
  DRAFT
  POSTED
  CANCELLED
}

enum AccountType {
  CASH_AND_BANK
  ACCOUNTS_RECEIVABLE
  INVENTORY
  OTHER_CURRENT_ASSETS
  FIXED_ASSETS
  ACCUMULATED_DEPRECIATION
  OTHER_ASSETS
  ACCOUNTS_PAYABLE
  OTHER_CURRENT_LIABILITIES
  LONG_TERM_LIABILITIES
  EQUITY
  REVENUE
  COGS
  EXPENSE
  OTHER_EXPENSE
  OTHER_INCOME
}

enum PrintTemplateType {
  ANGGARAN
  FAKTUR_PEMBELIAN
  FAKTUR_PENJUALAN
  JURNAL_UMUM
  PEKERJAAN_PESANAN
  PEMBAYARAN
  PEMBAYARAN_PEMBELIAN
  PEMINDAHAN_BARANG
  PENAMBAHAN_BAHAN_BAKU
  PENAWARAN_PENJUALAN
  PENCATATAN_BEBAN
  PENERIMAAN
  PENERIMAAN_BARANG
  PENERIMAAN_PENJUALAN
  PENGIRIMAN_PESANAN
  PENYELESAIAN_PESANAN
